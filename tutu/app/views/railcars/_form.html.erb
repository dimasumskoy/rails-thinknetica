<%= form_with(model: railcar, local: true) do |form| %>
  <%= render 'common/errors', resource: railcar %>
  
  <div class="field">
    <%= form.label :category %>
    <%= form.text_field :category, id: :railcar_category %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
